<?xml version="1.0"?>
<ESBEndpoint xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Id>dcb623b7-22e6-43ca-a234-1fe96a1ef20c</Id>
  <Version>006a326a-0cce-4a18-bb95-ce2b4839dc55</Version>
  <BusinessFlows />
  <Description>This adaptor endpoint provides an administrative file submission mechanism.  All messages should be in Neo Native form.  The file location is governed by the Global Environment Variable AdministrativeFileInputDirectory.</Description>
  <Name>File Submission Endpoint</Name>
  <Group>General</Group>
  <IsSystemObject>false</IsSystemObject>
  <IsPrivate>false</IsPrivate>
  <Bindings />
  <FarmSingleInstance>false</FarmSingleInstance>
  <Enabled>true</Enabled>
  <Zone>Enterprise</Zone>
  <AvailabilityGroupId>Neuron ESB Default Host</AvailabilityGroupId>
  <IncludeMetadata>true</IncludeMetadata>
  <AdapterName>File</AdapterName>
  <AdapterAssembly>Neuron.Esb.Adapters.FileAdapter.dll</AdapterAssembly>
  <AdapterMessageDirection>Duplex</AdapterMessageDirection>
  <AdapterClass>Neuron.Esb.Adapters.FileAdapter</AdapterClass>
  <AdapterMode>Publish</AdapterMode>
  <ProcessMode>false</ProcessMode>
  <ClientProcessMode>Messaging</ClientProcessMode>
  <ProcessList />
  <ServiceRateEnable>false</ServiceRateEnable>
  <Properties>
    <NameValuePair>
      <Name>Folder</Name>
      <Value>C:\FileIn</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileType</Name>
      <Value>*.xml</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileDetection</Name>
      <Value>FolderEvents</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>PollingInterval</Name>
      <Value>60</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileSortByField</Name>
      <Value>None</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileSortOrderField</Name>
      <Value>Ascending</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileAgeFilter</Name>
      <Value>00:00:00</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>SendTopic</Name>
      <Value>Enrich_LIMS_MaterialLot</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>RemoveBOM</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>SuppressFailedExtensions</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ProcessZeroBytes</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>MetadataOnly</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ArchiveOnGet</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ArchiveFolder</Name>
      <Value />
    </NameValuePair>
    <NameValuePair>
      <Name>ArchiveCreateSubfolders</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>RecursiveGet</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>OverrideEncoding</Name>
      <Value>Default</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ErrorMode</Name>
      <Value>Error</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ErrorHandling</Name>
      <Value>StopPollingOnError</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>CreateFolderPath</Name>
      <Value>True</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileEncoding</Name>
      <Value>Default</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileWriteOption</Name>
      <Value>Create</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>DisableWriteThruCache</Name>
      <Value>False</Value>
    </NameValuePair>
  </Properties>
  <AdapterBindings>
    <ExpressionBoundProperty>
      <PropertyName>Folder</PropertyName>
      <Expression>{$AdministrativeFileInputDirectory}</Expression>
    </ExpressionBoundProperty>
  </AdapterBindings>
  <PartyId>NeoPLM File Based Publisher</PartyId>
  <RunAs />
  <AdapterPolicy>Default</AdapterPolicy>
  <PoolSize>1</PoolSize>
  <ProcessingThreads>1</ProcessingThreads>
</ESBEndpoint>