	<xsd:complexType name="MaterialInformationType">
		<xsd:sequence>
			<xsd:element name="ID" type="IdentifierType" nillable="true" minOccurs="0"/>
			<xsd:element name="Description" type="DescriptionType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
			<!-- Location ELEMENT IS DEPRECATED and may be removed in a future release, use HierarchyScope instead -->
			<xsd:element name="Location" type="LocationType" minOccurs="0"/>
			<xsd:element name="HierarchyScope" type="HierarchyScopeType" nillable="true" minOccurs="0"/>
			<xsd:element name="PublishedDate" type="PublishedDateType" nillable="true" minOccurs="0"/>
			<xsd:element name="MaterialClass" type="MaterialClassType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
