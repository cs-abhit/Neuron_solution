<?xml version="1.0"?>
<ESBServicePolicy xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Id>d58d61cf-8c35-4312-81b5-c16c925d1636</Id>
  <Version>bb40859b-eff8-448e-b3c8-96ed6361261a</Version>
  <BusinessFlows />
  <Description>This Policy is used by any Service Proxy Services.  Currently there are two, the Neo Global Integration Service and the Neo Local Integration Service.  Note that based upon the current architecture, the only applicable tab is the REST Tab.  All others are meaningless, and should be ignored.</Description>
  <Name>Service Proxy Policy</Name>
  <Group>General</Group>
  <IsSystemObject>false</IsSystemObject>
  <IsPrivate>false</IsPrivate>
  <OnErrorAuditToDatabase>false</OnErrorAuditToDatabase>
  <OnErrorAuditToFile>false</OnErrorAuditToFile>
  <OnErrorRepublishMessage>false</OnErrorRepublishMessage>
  <RepublishTopic />
  <OnErrorRollback>false</OnErrorRollback>
  <AlwaysAvailable>true</AlwaysAvailable>
  <RequestReplyTimeout>60</RequestReplyTimeout>
  <DoNotApplyPolicyIfFaultContract>false</DoNotApplyPolicyIfFaultContract>
  <RetryCount>1</RetryCount>
  <RetryInterval>1</RetryInterval>
  <RollbackDelaySeconds>60</RollbackDelaySeconds>
  <RollbackMessageExpirationSeconds>300</RollbackMessageExpirationSeconds>
  <AuditDirectoryLocation />
  <InlcudeAsExceptions>false</InlcudeAsExceptions>
  <StartAndStopAdapterAccordingToAvailability>true</StartAndStopAdapterAccordingToAvailability>
  <HttpStatusCodes>
    <NeuronHttpStatusCode>
      <HttpStatusCode>200</HttpStatusCode>
      <Description>OK</Description>
    </NeuronHttpStatusCode>
    <NeuronHttpStatusCode>
      <HttpStatusCode>201</HttpStatusCode>
      <Description>Created</Description>
    </NeuronHttpStatusCode>
    <NeuronHttpStatusCode>
      <HttpStatusCode>202</HttpStatusCode>
      <Description>Accepted</Description>
    </NeuronHttpStatusCode>
    <NeuronHttpStatusCode>
      <HttpStatusCode>204</HttpStatusCode>
      <Description>No Content</Description>
    </NeuronHttpStatusCode>
  </HttpStatusCodes>
  <CustomHttpStatusCodes />
  <SoapFaultDescription />
</ESBServicePolicy>