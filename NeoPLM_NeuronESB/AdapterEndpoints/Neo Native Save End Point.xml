<?xml version="1.0"?>
<ESBEndpoint xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Id>989772f9-f4c7-4a90-b269-57e1d8775cb7</Id>
  <Version>c92817e6-0722-416f-a500-3704c0748b75</Version>
  <BusinessFlows />
  <Description>This adaptor endpoint saves all valid messages, messages that pass schema validation, in Neo Native format.  The file location is governed by the Global Enviroment Variable AcceptedMessagesDirectory.</Description>
  <Name>Neo Native Save End Point</Name>
  <Group>General</Group>
  <IsSystemObject>false</IsSystemObject>
  <IsPrivate>false</IsPrivate>
  <Bindings />
  <FarmSingleInstance>false</FarmSingleInstance>
  <Enabled>true</Enabled>
  <Zone>Enterprise</Zone>
  <AvailabilityGroupId>Neuron ESB Default Host</AvailabilityGroupId>
  <IncludeMetadata>true</IncludeMetadata>
  <AdapterName>File</AdapterName>
  <AdapterAssembly>Neuron.Esb.Adapters.FileAdapter.dll</AdapterAssembly>
  <AdapterMessageDirection>Duplex</AdapterMessageDirection>
  <AdapterClass>Neuron.Esb.Adapters.FileAdapter</AdapterClass>
  <AdapterMode>Subscribe</AdapterMode>
  <ProcessMode>false</ProcessMode>
  <ClientProcessMode>Messaging</ClientProcessMode>
  <ProcessList />
  <ServiceRateEnable>false</ServiceRateEnable>
  <Properties>
    <NameValuePair>
      <Name>Folder</Name>
      <Value />
    </NameValuePair>
    <NameValuePair>
      <Name>FileType</Name>
      <Value>txt</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileDetection</Name>
      <Value>FolderEvents</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>PollingInterval</Name>
      <Value>60</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileSortByField</Name>
      <Value>None</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileSortOrderField</Name>
      <Value>Ascending</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileAgeFilter</Name>
      <Value>00:00:00</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>SendTopic</Name>
      <Value />
    </NameValuePair>
    <NameValuePair>
      <Name>RemoveBOM</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>SuppressFailedExtensions</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ProcessZeroBytes</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>MetadataOnly</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ArchiveOnGet</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ArchiveFolder</Name>
      <Value />
    </NameValuePair>
    <NameValuePair>
      <Name>ArchiveCreateSubfolders</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>RecursiveGet</Name>
      <Value>False</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>OverrideEncoding</Name>
      <Value>Default</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ErrorMode</Name>
      <Value>Error</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>ErrorHandling</Name>
      <Value>StopPollingOnError</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>CreateFolderPath</Name>
      <Value>True</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileEncoding</Name>
      <Value>Default</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>FileWriteOption</Name>
      <Value>Overwrite</Value>
    </NameValuePair>
    <NameValuePair>
      <Name>DisableWriteThruCache</Name>
      <Value>False</Value>
    </NameValuePair>
  </Properties>
  <AdapterBindings>
    <ExpressionBoundProperty>
      <PropertyName>Folder</PropertyName>
      <Expression>{$AcceptedMessagesDirectory}</Expression>
    </ExpressionBoundProperty>
  </AdapterBindings>
  <PartyId>SaveNeoNativeMessages</PartyId>
  <RunAs />
  <AdapterPolicy>Default</AdapterPolicy>
  <PoolSize>1</PoolSize>
  <ProcessingThreads>1</ProcessingThreads>
</ESBEndpoint>